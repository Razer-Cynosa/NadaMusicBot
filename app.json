{
  "name": "NadaMusicBot",
  "description": "Saya bisa memutar musik di panggilan suara grub anda.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/Razer-Cynosa/NadaMusicBot",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "isi dengan bot token dari @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "Nama MusicPlayer Bot.",
      "required": false,
      "value": ""
    },
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "ini untuk siapa aja yang bisa control bot mu ya asu.",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Maksimal durasi desahanmu (menit).",
      "required": true,
      "value": "10"
    }
  }
}
